{
  "compilerOptions": {
    "declaration": true,
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "outDir": "lib",
    "rootDir": "src",
    "skipLibCheck": false,
    "strict": true,
    "strictNullChecks": true,
    "target": "es5"
  }
}
